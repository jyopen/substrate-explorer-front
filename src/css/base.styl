
.page {
  width: 1180px;
}


.single-line {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}


@media screen and (max-width: 768px) {
  .page {
    width 96%
  }
}

@media screen and (min-width: 769px) and (max-width: 1200px) {
  .page {
    width 94%
  }
}
